<!--
Purpose: This include file is to override sitecore web.config configurations, 
keeping web.config as close to sitecore's clean copy as possible
-->

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>     

        <!-- CACHE SIZES -->
        <cacheSizes>
            <sites>
                <TresDivas>
                    <html>50MB</html>
                    <registry>0</registry>
                    <viewState>0</viewState>
                    <xsl>25MB</xsl>
                </TresDivas>
            </sites>
        </cacheSizes>
    </sitecore>
</configuration>
